{
  "name": "wp-starter",
  "version": "1.0.0",
  "description": "webpack wp starter",
  "main": "config/webpack.dev.js",
  "engines": {
    "node": ">=8.5.0"
  },
  "browserslist": [
    "last 3 versions",
    "ie >= 10"
  ],
  "scripts": {
    "start": "webpack --watch --config config/webpack.dev.js",
    "dev": "webpack --config config/webpack.dev.js",
    "prod": "NODE_ENV='production' webpack --config config/webpack.prod.js",
    "css": "node-sass --include-path ./node_modules ./wp-content/themes/smartboard/assets/scss/site.scss ./wp-content/themes/smartboard/assets/dist/site.css --source-map-embed",
    "watch-css": "nodemon -e scss -x npm run css"
  },
  "dependencies": {
    "animejs": "^2.2.0",
    "aos": "^2.2.0",
    "bulma": "^0.7.2",
    "detect-browser": "^2.4.0",
    "in-viewport": "^3.6.0",
    "jquery": "^3.3.1",
    "lodash.foreach": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "smooth-scroll": "^12.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/plugin-transform-block-scoping": "^7.0.0-beta.44",
    "@babel/plugin-transform-classes": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.42",
    "autoprefixer": "^7.2.4",
    "babel-loader": "^8.0.0-beta.2",
    "babel-polyfill": "^6.26.0",
    "browser-sync": "^2.23.5",
    "browser-sync-webpack-plugin": "^1.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "closest": "0.0.1",
    "css-loader": "^0.28.11",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "kss": "^3.0.0-beta.18",
    "kss-webpack-plugin": "^1.3.1",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.3",
    "onchange": "^3.3.0",
    "postcss-loader": "^2.1.3",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.7",
    "script-loader": "^0.7.2",
    "store": "^2.0.12",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-dashboard": "^1.0.2",
    "webpack-merge": "^4.1.2",
    "webpack-notifier": "^1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/webpack-wp-starter.git"
  },
  "keywords": [
    "wordpress",
    "lando",
    "docker",
    "webpack"
  ],
  "author": "crac.io",
  "license": "ISC"
}
